{
  "name": "@dannyxcii/create-modula-app",
  "version": "0.2.0",
  "scripts": {
    "dev": "webpack --config webpack.config.js --mode=development",
    "build": "webpack --config webpack.config.js --mode=production",
    "watch": "webpack --config webpack.config.js --mode=development --watch",
    "start-api": "node api/api.js",
    "start-client": "cd ./public && npx serve",
    "serve": "concurrently \"npm:watch\" \"npm:start-api\" \"npm:start-client\""
  },
  "bin": {
    "create-modula-app": "installer/installer"
  },
  "devDependencies": {
    "@dannyxcii/badger": "^0.4.2",
    "concurrently": "^8.2.2",
    "css-loader": "~7.1.2",
    "css-minimizer-webpack-plugin": "~7.0.0",
    "glob": "~10.4.1",
    "ignore-emit-webpack-plugin": "~2.0.6",
    "sass": "~1.77.2",
    "sass-loader": "~14.2.1",
    "style-loader": "~4.0.0",
    "terser-webpack-plugin": "~5.3.10",
    "ts-loader": "~9.5.1",
    "typescript": "~5.4.5",
    "webpack": "~5.91.0",
    "webpack-cli": "~5.1.4"
  },
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "@dannyxcii/modula": "~0.2.0",
    "chalk": "~5.3.0",
    "commander": "~12.1.0",
    "express": "^4.19.2",
    "node-fetch": "^3.3.2"
  }
}
